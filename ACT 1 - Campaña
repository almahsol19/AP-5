using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Enter Number:");


        string input = Console.ReadLine();

        if (input == "0")
        {
            Console.WriteLine("L");
            Console.WriteLine("True");
        }
        else if (input == "1")
        {
            Console.WriteLine("H");
            Console.WriteLine("False");
        }
        else
        {
            Console.WriteLine("Invalid input. Please enter 0 or 1.");
        }
    }
}
