using System;
using System.Threading;

public class BlinkLED
{
    public static void Main(string[] args)
    {
        for (int i = 0; i < 5; i++)
        {
            Console.WriteLine("LED ON");
            Thread.Sleep(500); // Wait for 500 milliseconds
            Console.WriteLine("LED OFF");
            Thread.Sleep(500); // Wait for 500 milliseconds
        }
    }
}
